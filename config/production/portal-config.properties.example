#Set ISO 639-1 code for supported languages
i18n.supportedLanguages=en,fr

#Set ISO 639-1 code for default language in case it was unspecified by the request (or the browser).
#If omitted, English will be used.
i18n.defaultLanguage=en

#Do we use the minified version of JavaScript and CSS files. Always use minified when using versioning.
url.useMinified=true

#GoogleAnalytics variables, if you don't want to use it, just remove the next 2 lines
googleanalytics.siteVerification=YOUR_VALUE
googleanalytics.account=YOUR_VALUE

#Database
database.driver=org.postgresql.Driver
database.url=jdbc:postgresql://YOUR_DATABASE_SERVER/dataportal
database.user=YOUR_VALUE
database.password=YOUR_VALUE
database.idleConnectionTestPeriod=60
hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
hibernate.show_sql=false
hibernate.jdbc.fetch_size=1000

#Email sender configurations
email.smtpServer=YOUR_VALUE
email.port=25
#should we hash the email or store the plain text version
email.hashAddress=true
#if we hash it, should we 'salt' it? (ignored if email.hashAddress=false)
email.salt=6SFDW$PTkp04yogq>vyEfku

# Optional, if you want to overwrite the location of the Freemarker email templates.
# email.templateFolder=/WEB-INF/email/

#The URL of the tile server
tiler.protocol=http
tiler.domain=tiles.canadensys.net
tiler.port=80

#The URL of your feedback system. Could also be a mailto or can be removed if no feedback is required.
feedback.url=http://code.google.com/p/canadensys/issues/entry?template=Explorer%20-%20Interface%20issue

#The path of the folder to store the generated content. Make sure system user running Tomcat can read/write in this folder.
searchServiceConfig.generatedContentFolder=YOUR_VALUE
#The public URL to reach the generated content. Must ends with a slash.
searchServiceConfig.publicDownloadURL=YOUR_VALUE
#From email to use to send emails
searchServiceConfig.emailFrom=YOUR_VALUE

